container.style.cursor='grab';});container.addEventListener('mouseup',function(){isDown=false;container.style.cursor='grab';});container.addEventListener('mousemove',function(e){if(!isDown)return;e.preventDefault();var x=e.pageX-startX;var y=e.pageY-startY;panX=x;panY=y;updateTransform();});container.addEventListener('wheel',function(e){e.preventDefault();var delta=e.deltaY*-0.001;scale=Math.min(Math.max(0.1,scale+delta),4);updateTransform();});function updateTransform(){content.style.transform='translate('+panX+'px, '+panY+'px) scale('+scale+')';}resizer.addEventListener('mousedown',function(e){isResizing=true;startY_Resize=e.clientY;startHeight=rulesPanel.offsetHeight;document.body.style.cursor='ns-resize';});document.addEventListener('mousemove',function(e){if(!isResizing)return;var dy=startY_Resize-e.clientY;var newHeight=startHeight+dy;if(newHeight>50&&newHeight<window.innerHeight-100){rulesPanel.style.height=newHeight+'px';}});document.addEventListener('mouseup',function(){if(isResizing){isResizing=false;document.body.style.cursor='default';}});</script>
